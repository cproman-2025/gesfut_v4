
rules_version = '2';

service cloud.firestore {
  match /databases/{database}/documents {
    // This rule allows anyone with your project's credentials to read, write,
    // and delete all data in your Firestore database. It is not secure.
    // Use this for early development, but lock it down before production.
    match /{document=**} {
      allow read, write: if true;
    }
  }
}
